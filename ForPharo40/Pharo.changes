dsFor: 'tests' stamp: 'DamienCassou 9/2/2015 09:08'!testCreateScriptForLaTeXCompilation	| configuration phase script |	self flag: #todo.	"This test should better be part of the Pillar-Tests-ExporterLaTeX. But this will wait for the transformer/phase concept merge."	configuration := self fromString: '{ #outputType: #latex }'.	script := configuration baseDirectory / 'pillarPostExport.sh'.	script ensureDelete.	phase := self actualClass new configuration: configuration.	self deny: script exists.	phase transformerLaTeXCompilationOf: {}.	self assert: script exists.	script ensureDelete! !!PRExportPhaseTest methodsFor: 'tests' stamp: 'CyrilFerlicot 10/29/2015 22:00'!testDefaultTemplate	| output bindings |	bindings := PRPillarConfiguration new		propertyAt: 'content' put: 'chapeau';		yourself.	output := self actualClass new expandTemplateWithBindings: bindings.	self assert: output equals: 'chapeau'! !!PRExportPhaseTest methodsFor: 'tests' stamp: 'CyrilFerlicot 6/15/2015 14:48'!testDocumentPropertiesAreAccessible	| doc data |	doc := PRDocument new propertyAt: 'authors' put: 'Someone'; yourself.	data := self actualClass new extractDataFrom: doc using: PRMockWriter new.	self assert: (data hasProperty: 'authors').	self assert: (data propertyAt: 'authors') equals: 'Someone'! !!PRExportPhaseTest methodsFor: 'tests' stamp: 'CyrilFerlicot 10/29/2015 22:00'!testDocumentPropertiesCanBeReferencedFromTemplates	| doc data conf output |	doc := PRDocument new		propertyAt: 'authors' put: 'Someone';		yourself.	conf := PRPillarConfiguration new		templateStream: 'Authors: {{{authors}}}';		yourself.	output := String		streamContents: [ :outStream | 			conf outputFile: outStream.			data := self actualClass new configuration: conf; write: doc using: PRMockWriter new ].	self assert: output equals: 'Authors: Someone'! !!PRExportPhaseTest methodsFor: 'tests' stamp: 'CyrilFerlicot 10/29/2015 22:00'!testDocumentPropertiesTakePrecedence	| doc data conf |	doc :=